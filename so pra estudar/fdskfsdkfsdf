[32m+[m[32m    r'(?=.*[ -\/:-@[-`{-~])'[m
[32m+[m[32m    r'.{12,}'[m
[32m+[m[32m    r'$',[m
[32m+[m[32m    flags=re.M[m
[32m+[m[32m)[m
