[32m+[m[32m    ^[m
[32m+[m[32m    (?:[m
[32m+[m[32m        25[0-5]| # 250 - 255[m
[32m+[m[32m        2[0-4][0-9]| # 200 - 249[m
[32m+[m[32m        1[0-9]{2} # 100 - 199[m
[32m+[m[32m        1[1-9][0-9] # 10 - 99[m
[32m+[m[32m        [0-9] # 0 - 9[m
[32m+[m[32m    ){3}[m
[32m+[m[32m    .\[m
[32m+[m[32m         (?:[m
[32m+[m[32m        25[0-5]| # 250 - 255[m
[32m+[m[32m        2[0-4][0-9]| # 200 - 249[m
[32m+[m[32m        1[0-9]{2} # 100 - 199[m
[32m+[m[32m        1[1-9][0-9] # 10 - 99[m
[32m+[m[32m        [0-9] # 0 - 9[m
[32m+[m[32m    ){4}[m
[32m+[m[32m    .\[m
[32m+[m[32m         (?:[m
[32m+[m[32m        25[0-5]| # 250 - 255[m
[32m+[m[32m        2[0-4][0-9]| # 200 - 249[m
[32m+[m[32m        1[0-9]{2} # 100 - 199[m
[32m+[m[32m        1[1-9][0-9] # 10 - 99[m
[32m+[m[32m        [0-9] # 0 - 9[m
[32m+[m[32m    )[m
[32m+[m[32m    .\[m[41m [m
[32m+[m[32m     (?:[m
[32m+[m[32m        25[0-5]| # 250 - 255[m
[32m+[m[32m        2[0-4][0-9]| # 200 - 249[m
[32m+[m[32m        1[0-9]{2} # 100 - 199[m
[32m+[m[32m        1[1-9][0-9] # 10 - 99[m
[32m+[m[32m        [0-9] # 0 - 9[m
[32m+[m[32m    )[m
[32m+[m[32m    \b[m
[32m+[m[32m    $[m
[32m+[m[32m''', flags=re.X)[m
[32m+[m
[32m+[m
