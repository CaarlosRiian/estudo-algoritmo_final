[32m+[m[32mdef zero_a_nove():[m
[32m+[m[32m    return chr(randint(48, 57))[m
[32m+[m
[32m+[m[32mdef a_a_z():[m
[32m+[m[32m    return chr(randint(97, 122))[m
[32m+[m
[32m+[m[32mdef A_a_Z():[m
[32m+[m[32m    return chr(randint(65, 90))[m
[32m+[m
[32m+[m[32mdef strange_chars():[m
[32m+[m[32m    rand_range = [[m[41m [m
[32m+[m[32m        randint(32, 47),[m
[32m+[m[32m        randint(58, 64),[m[41m [m
[32m+[m[32m        randint(91,96),[m
[32m+[m[32m        randint(123, 126),[m
[32m+[m[32m    ][m
[32m+[m
[32m+[m[32m    return chr(choice(rand_range))[m
[32m+[m[41m     [m
[32m+[m[32mdef create_pass([m
[32m+[m[32m    length = 12,[m
[32m+[m[32m    upper = True,[m
[32m+[m[32m    lower=True,[m
[32m+[m[32m    numbers=True,[m
[32m+[m[32m    chars=True[m
[32m+[m[32m):[m
[32m+[m[32m    password = [][m
[32m+[m
[32m+[m[32m    for i in range(length):[m
[32m+[m[32m        if numbers:[m
[32m+[m[32m            password.append(zero_a_nove())[m
[32m+[m[32m        if lower:[m
[32m+[m[32m            password.append(a_a_z())[m
[32m+[m[32m        if upper:[m
[32m+[m[32m            password.append(A_a_Z())[m
[32m+[m[32m        if chars:[m
[32m+[m[32m            password.append(strange_chars())[m
[32m+[m
[32m+[m[32m    password = password[:length][m
[32m+[m[32m    shuffle(password)[m
[32m+[m[32m    return ''.join(password)[m
