[32m+[m
[32m+[m[32mprint(string)[m
[32m+[m[32mprint(re.search(r'teste', string))[m
[32m+[m[32mprint(re.findall(r'teste', string))[m
[32m+[m[32mprint(re.sub(r'teste', 'ACDB' , string))[m
[32m+[m
[32m+[m[32mregexp = re.compile(r'teste')[m
[32m+[m[32mprint(regexp.search(string))[m
[32m+[m[32mprint(regexp.findall(string))[m
[32m+[m[32mprint(regexp.sub('ADC', string))[m
